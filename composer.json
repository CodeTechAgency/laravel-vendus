{
  "name": "codetech/laravel-vendus",
  "description": "Laravel wrapper for the codetech/vendus-api package.",
  "keywords": [
    "vendus",
    "faturação",
    "faturas",
    "recibos",
    "laravel-vendus",
    "laravel",
    "codetech"
  ],
  "authors": [
    {
      "name": "José Osório",
      "email": "jfrosorio@gmail.com",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
    "codetech/vendus-api": "^1.1.0"
  },
  "autoload": {
    "psr-4": {
      "CodeTech\\Vendus\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "laravel": {
      "providers": [
        "CodeTech\\Vendus\\Providers\\VendusServiceProvider"
      ]
    }
  }
}
